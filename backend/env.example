# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration (MongoDB Atlas - Free Tier)
MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/babyllm?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random
JWT_EXPIRE=30d

# API Keys (Free Tiers)
BING_SEARCH_API_KEY=your-bing-search-api-key
OPENAI_API_KEY=your-openai-api-key

# OpenRouter API (Alternative free LLM provider)
OPENROUTER_API_KEY=your-openrouter-api-key

# CORS Configuration
CORS_ORIGIN=http://localhost:19006,https://babyllm.vercel.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Puppeteer Configuration
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium-browser
