<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="auth_prefs.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- Include all data for device transfer -->
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>
